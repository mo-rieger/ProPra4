<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gens.ca.cookiemonster.CookieMonsterGenController">
       <children>
      <Button fx:id="buttonGenerate" layoutX="136.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleGenerate" text="Generate" />
      <Label layoutX="133.0" layoutY="22.0" text="Cells per row" />
      <Label layoutX="385.0" layoutY="22.0" text="Generations" />
      <Label layoutX="134.0" layoutY="104.0" text="Cell size (px)" />
      <TextField fx:id="textFieldCellSize" layoutX="147.0" layoutY="130.0" prefHeight="26.0" prefWidth="57.0" />
      <TextField fx:id="textFieldCells" layoutX="90.0" layoutY="52.0" />
      <TextField fx:id="textFieldGens" layoutX="340.0" layoutY="52.0" />
      <Slider fx:id="sliderStates" blockIncrement="1.0" layoutX="355.0" layoutY="155.0" majorTickUnit="20.0" max="20.0" min="1.0" minorTickCount="1" showTickLabels="true" />
      <Label layoutX="404.0" layoutY="129.0" text="States" />
      <TextField fx:id="textFieldStates" layoutX="384.0" layoutY="188.0" prefHeight="26.0" prefWidth="80.0" />
      <Label layoutX="144.0" layoutY="186.0" text="Step (ms)" />
      <TextField fx:id="textFieldStep" layoutX="90.0" layoutY="209.0" />
      <Button fx:id="buttonStop" layoutX="395.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleStop" text="Stop" />
   </children>
</AnchorPane>
